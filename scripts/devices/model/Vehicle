import("devices.Device")
//import("core.ACLEvaluator")

function Vehicle() {
  
  	this.running = false
  
	Device.apply(this, arguments)

	this.start = function(name) {
//      	      if (!ACLEvaluator.hasAccess(request.user, this.startVehicleACL)) return null
		this.running = "true"
        this.save()
	},
	
	this.stop = function() {
		this.running = "false"
        this.save()
	}
}
